<h1>Tvs#new</h1>
<div class="row justify-content-center">
    <%= form_with model: @tv do |form|%>
        <div class="form-group">
          <%=form.label :Name%>
          <%= form.text_field :name, class: 'form-control'%>
        </div> 
        <div class="form-group">
          <%= form.label :Company%>
          <%= form.text_field :company, class: 'form-control'%>
        </div>
        <div class="form-group">
          <%= form.label :Price%>
          <%= form.text_field :price, class: 'form-control'%>
        </div>
        <div class="form-group">
          <%= form.label :Model%>
          <%= form.text_field :model, class: 'form-control'%>
        </div><br>
        <div class="form-group">
          <%= form.submit class: 'form-control btn-primary'%>
        </div>
    <%end%>
</div>    
