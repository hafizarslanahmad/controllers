<h1>Bags#new</h1>
<div class="row justify-content-center">
    <%= form_with model: @bag do |form|%>
        <div class="form-group">
          <%=form.label :Book%>
          <%= form.text_field :book, class: 'form-control'%>
        </div> 
        <div class="form-group">
          <%= form.label :Register%>
          <%= form.text_field :register, class: 'form-control'%>
        </div>
        <div class="form-group">
          <%= form.label :Pencil%>
          <%= form.text_field :pencil, class: 'form-control'%>
        </div><br>
        <div class="form-group">
          <%= form.submit class: 'form-control btn-primary'%>
        </div>
    <%end%>
</div>    


