<h1>Autos Create Data</h1>
<div class="row justify-content-center">
    <%= form_with model: @auto do |form| %>
      <div class="form-group">
        <%= form.label :Vehical %>
        <%= form.text_field :vehical, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= form.label :Color %>
        <%= form.text_field :color, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= form.label :Company %>
        <%= form.text_field :company, class: 'form-control' %>
      </div>
      <br>
      <div class="form-group">
        <%= form.submit class: 'form-control btn-primary' %>
      </div>
    <% end %>
  </div>
